<h1>Books#new</h1>
<p>Find me in app/views/books/new.html.erb</p>
<h2>New Book</h2>


  
<%= form_with model:@book, local:true do |f| %>

  <% if @book.errors.any? %>
  <div>
    <ul>
      <% @book.errors.full_messages.each do |message| %>
      <li><%= message %></li>
      <% end %>
    </ul>
  </div>
  <% end %>

  <h4>Title</h4>
  <%= f.text_field :title %>
  <h4>Body</h4>
  <%= f.text_area :body %>
  <%= f.submit 'Creat Book' %>
<% end %>